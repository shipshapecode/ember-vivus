    "ember-addon",
    "animation",
    "svg",
    "vivus"
  ],
  "license": "MIT",
  "author": {
    "name": "Robert Wagner",
    "email": "rwwagner90@gmail.com",
    "url": "https://github.com/rwwagner90"
  },
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": "https://github.com/shipshapecode/ember-vivus",
  "scripts": {
