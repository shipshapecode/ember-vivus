  "greenkeeper": {
    "ignore": [
      "broccoli-asset-rev",
      "ember-cli-babel",
      "ember-cli-htmlbars",
      "ember-cli-htmlbars-inline-precompile",
      "ember-cli-qunit",
      "ember-cli-sri",
      "ember-export-application-global",
      "ember-resolver",
      "loader.js"
